{
  "name": "windsurf-status",
  "displayName": "windsurf-status",
  "description": "Shows notifications inside Windsurf when the service status changes",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.106.0"
  },
  "categories": ["Other"],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.106.0",
    "@types/node": "22.x",
    "typescript": "^5.9.3"
  }
}
